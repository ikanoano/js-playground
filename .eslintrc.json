{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "amd": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "jest"
  ],
  "rules": {
    "indent": ["error", 2]
  }
}